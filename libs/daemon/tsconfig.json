{
    "extends": "@makepad/tsconfig/tsconfig.json",
    "exclude": [
      "./node_modules/",
      "./dist/"
    ],
    "include": ["./src/"],
    "compilerOptions": {
      "target": "ES2022",
      "rootDir": "./src",
      "outDir": "./dist",
      "declaration": true,
      "strict": true,
      "noUnusedLocals": false,
      "noUnusedParameters": false
    }
  }